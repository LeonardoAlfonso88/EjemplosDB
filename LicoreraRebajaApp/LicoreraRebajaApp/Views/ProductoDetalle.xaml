<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LicoreraRebajaApp.Views.ProductoDetalle">
    <ContentPage.Content>
        <StackLayout>
            <Label StyleClass="label" Text="{Binding IdProducto}"></Label>
            <Label StyleClass="label" Text="{Binding Nombre}"></Label>
            <Label StyleClass="label" Text="{Binding ValorUnitario}"></Label>
            <Label StyleClass="label" Text="{Binding Tamaño}"></Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>